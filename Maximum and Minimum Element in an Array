#include <bits/stdc++.h>

using namespace std;

int main()
{
  int a[10]={1,2,4,6,8,2,29,7,8,9};
  int n=10;
  int maxx=INT_MIN;
  int minn=INT_MAX;
  for(int i=0;i<n;i++)
  {
      if(a[i]>maxx)
      {
          maxx=a[i];
      }
      
      if(a[i]<minn)
      {
          minn=a[i];
      }
  }

cout<<maxx<<" ";
cout<<minn<<" ";
    return 0;
}
